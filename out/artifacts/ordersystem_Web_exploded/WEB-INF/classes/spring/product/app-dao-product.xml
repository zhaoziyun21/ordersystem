<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"  
    xsi:schemaLocation="  
	http://www.springframework.org/schema/beans   
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd  
	http://www.springframework.org/schema/context   
	http://www.springframework.org/schema/context/spring-context-3.0.xsd"> 
	
	<!-- 产品 -->
	<bean id="xProductsDao" class="com.kssj.product.dao.impl.XProductsDaoImpl" parent="genericDao"/>
	<!-- 产品类别 -->
	<bean id="xProCategoryDao" class="com.kssj.product.dao.impl.XProCategoryDaoImpl" parent="genericDao"/>
	<!-- 产品订购详情 -->
	<bean id="xProOrderDetailDao" class="com.kssj.product.dao.impl.XProOrderDetailDaoImpl" parent="genericDao"/>
	<!-- 收货人 -->
	<bean id="xReceiverInfoDao" class="com.kssj.product.dao.impl.XReceiverInfoDaoImpl" parent="genericDao"/>
	<!-- 产品后台管理 -->
	<bean id="xProductsManageDao" class="com.kssj.product.dao.impl.XProductsManageDaoImpl" parent="genericDao"/>
	<!-- 产品审批 -->
	<bean id="xProExamineDao" class="com.kssj.product.dao.impl.XProExamineDaoImpl" parent="genericDao"/>
</beans>