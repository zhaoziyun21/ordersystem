<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kssj.order.model.XNoticeBill" table="x_notice_bill">
        <id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="dictionaryId" type="java.lang.String">
            <column name="dictionary_id" length="32" />
        </property>
        <property name="noticeId" type="java.lang.String">
            <column name="notice_id" />
        </property>
        <property name="noticeName" type="java.lang.String">
            <column name="notice_name" />
        </property>
        <property name="insTime" type="java.util.Date">
            <column name="ins_time">
            </column>
        </property>
        <property name="insUser" type="java.lang.String">
            <column name="ins_user">
            </column>
        </property>
        <property name="updTime" type="java.util.Date">
            <column name="upd_time">
            </column>
        </property>
        <property name="updUser" type="java.lang.String">
            <column name="upd_user">
            </column>
        </property>
    </class>
</hibernate-mapping>
